<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>StudentController</servlet-name>
  	<display-name>StudentController</display-name>
  	<description></description>
  	<servlet-class>com.sathish.controller.StudentController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>RegisterUserController</servlet-name>
  	<display-name>RegisterUserController</display-name>
  	<description></description>
  	<servlet-class>com.sathish.controller.RegisterUserController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>UserLoginController</servlet-name>
  	<display-name>UserLoginController</display-name>
  	<description></description>
  	<servlet-class>com.sathish.controller.UserLoginController</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>StudentController</servlet-name>
  	<url-pattern>/StudentController</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>RegisterUserController</servlet-name>
  	<url-pattern>/RegisterUserController</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>UserLoginController</servlet-name>
  	<url-pattern>/UserLoginController</url-pattern>
  </servlet-mapping>
</web-app>
